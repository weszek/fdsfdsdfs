if game.PlaceId == 7211666966 then
local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Tower of Jumps", "BloodTheme")
  
local Tab = Window:NewTab("Options")
local Section = Tab:NewSection("Options")
Section:NewSlider("WalkSpeed", "WalkSpeed Changer", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)  
  
Section:NewSlider("JumpPower", "JumpPower Changer", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)  
Section:NewButton("Inf Jump", "Turn on Inf Jump", function()
    local InfiniteJumpEnabled = true game:GetService("UserInputService").JumpRequest:connect(function() 	if InfiniteJumpEnabled then 		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping") 	end end)
end)  


-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local ToggleUI = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

ToggleUI.Name = "ToggleUI"
ToggleUI.Parent = ScreenGui
ToggleUI.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ToggleUI.BackgroundTransparency = 0.500
ToggleUI.Position = UDim2.new(0, 0, 0.41852349, 0)
ToggleUI.Size = UDim2.new(0, 138, 0, 50)
ToggleUI.Font = Enum.Font.SourceSans
ToggleUI.Text = "Toggle"
ToggleUI.TextColor3 = Color3.fromRGB(0, 0, 0)
ToggleUI.TextSize = 14.000

-- Scripts:

local function XQNK_fake_script() -- ToggleUI.Script 
	local script = Instance.new('Script', ToggleUI)

	local toggleButton = script.Parent
	
	local function toggleUI()
		Library:ToggleUI()
	end
	
	toggleButton.MouseButton1Click:Connect(toggleUI)
end
coroutine.wrap(XQNK_fake_script)()
end
